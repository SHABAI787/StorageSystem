             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" Title="ПОИСК ТОВАРА" x:Name="Content">

        <StackLayout x:Name="StackL">
            <SearchBar x:Name="SearchB" Placeholder="Поиск..." SearchButtonPressed="SearchB_SearchButtonPressed" TextChanged="SearchB_TextChanged" />
        </StackLayout>

    </ContentPage>
    <ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" Title="ИСТОРИЯ" x:Name="Content2">

        <StackLayout x:Name="StackL2">
            
        </StackLayout>

    </ContentPage>
</TabbedPage>